<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude AI model files from backup (too large) -->
        <exclude domain="file" path="models/"/>
        <exclude domain="database" path="medical_cache.db"/>
    </cloud-backup>
    
    <device-transfer>
        <!-- Include user preferences and settings -->
        <include domain="sharedpref" path="."/>
        <include domain="file" path="user_data/"/>
    </device-transfer>
</data-extraction-rules>
